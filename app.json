{
  "expo": {
    "name": "monhealth",
    "slug": "monhealth",
    "scheme": "monhealth",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./public/assets/monhealth-splash.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./public/assets/monhealth-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#fff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./public/assets/monhealth-splash.png",
        "backgroundColor": "#fff"
      },
      "package": "com.monapp.monhealth"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./public/assets/favicon.png"
    },
    "plugins": ["expo-router"]
  }
}
